@model List<MVC_Full_Training.Models.Teacher>
<h2>GetTeachers</h2>

<table cellpadding="0" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>ID</th>
            <th>Teacher Name</th>
            <th>Teacher Number</th>
            <th>Delete</th>
        </tr>

    </thead>
    <tbody>
        @foreach (var teacher in Model)
        {
            <tr>
                <td valign="middle" data-header-title="Date"><span>@teacher.teacherID</span></td>
                <td valign="middle" data-header-title="Price"><span>@teacher.teacherName</span></td>
                <td valign="middle" data-header-title="Supplement"><span>@teacher.teacherNo</span></td>
                <td><a href="/Teacher/DeleteTeacher/@teacher.teacherID">Delete</a></td>
            </tr>
        }

    </tbody>

</table>